#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/neonctl@2.20.2/node_modules/neonctl/node_modules:/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/neonctl@2.20.2/node_modules:/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/neonctl@2.20.2/node_modules/neonctl/node_modules:/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/neonctl@2.20.2/node_modules:/mnt/c/AI-BOS/NEXIS-AFENDA-V4/afenda/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../neonctl/cli.js" "$@"
else
  exec node  "$basedir/../neonctl/cli.js" "$@"
fi
